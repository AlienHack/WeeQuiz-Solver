{
  "manifest_version": 2,

  "name": "WeeQuiz Solver 1.2",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "1.2",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [{
      "matches": ["http://*.weequizz.com/*"],
      "js": ["contentscript.js"]
  }],
  "web_accessible_resources": ["script.js"]
}